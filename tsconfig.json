{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "app/javascript/**/*"
  ]
}